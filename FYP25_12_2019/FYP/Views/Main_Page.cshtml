            $('#Subscribe').click(function (){
                var Valid = false;
                if (!($('#sub_email').val().trim() != '')) {
                    Valid = false;
                    $('#emty_eror').css('visibility', 'visible');
                    $('#emty_valid').css('visibility', 'hidden');
                }
                else {
                    $('#emty_eror').css('visibility', 'hidden');
                    var emailReg = /^([\w-\.]+@@([\w-]+\.)+[\w-]{2,4})?$/;
                    var emailval = $('#sub_email').val();

                    if (emailReg.test(emailval)) {
                        Valid = true;
                        $('#emty_valid').css('visibility', 'hidden');

                    }

                    else {
                        Valid = false;
                        $('#emty_valid').css('visibility', 'visible');
                    }

                }

                if (Valid) {
                    var subscribe_val = $('#sub_email').val();
                    debugger
                    $.ajax({
                        type: 'POST',
                        url: '/Subscribe/Index',
                        contentType: "application/json;charset=utf-8",
                        data: JSON.stringify({ subscribe: subscribe_val }),
                        success: function (data) {
                            if (data.status) {
                                $('#email_success').fadeIn("slow");
                                $('#email_success').fadeOut(3000);
                            }


                        }

                    })
                }


            });

            var value = '@Request.RequestContext.HttpContext.Session["Username"]';

            if (value != '') {
                $('.lgn').show();
                $('.nt_lgn').hide();
            }
            else {
                $('.nt_lgn').show();
                $('.lgn').hide();
            }


            $("#search").click(function () {
                var Valid = false;

                var search = $('#CityName').val();

                if (search != null) {
                    Valid = true;
                }
                else {
                    Valid = false;
                }

                debugger


                if (Valid) {
                    $.ajax({
                        type: 'POST',
                        url: '/Search/search',
                        contentType: "application/json;charset=utf-8",
                        data: JSON.stringify({ searchbar: search }),
                        success: function (data) {
                            if (data.status) {
                                window.location.href = (data.newurl);
                            }
                            else {
                                window.location.href = (data.cURL);
                            }

                        }

                    })
                }
                debugger
            });


            $("#CityName").autocomplete({
                source: function (request, response) {
                    $.ajax({
                        url: "/Search/GetRecord",
                        type: "POST",
                        dataType: "json",
                        data: { prefix: request.term },
                        success: function (data) {
                            response($.map(data, function (item) {

                                return { label: item.C_Name, value: item.C_Name };
                            }))
                        }
                    })
                },
            });



            $('#login').click(function () {
                var Valid = true;


                if (!($('#username').val().trim() != '')) {
                    Valid = false;
                    $('.username').css('visibility', 'visible');
                    $('.valid').css('visibility', 'hidden');
                    $('.v_email').css('visibility', 'hidden');
                }
                else {
                    $('.username').css('visibility', 'hidden');
                    var emailReg = /^([\w-\.]+@@([\w-]+\.)+[\w-]{2,4})?$/;
                    var emailval = $('#username').val();

                    if (emailReg.test(emailval)) {

                        $('.v_email').css('visibility', 'hidden');

                    }

                    else {
                        Valid = false;
                        $('.v_email').css('visibility', 'visible');
                    }

                }


                if (!($('#password').val().trim() != '')) {
                    Valid = false;
                    $('.lgn_password').css('visibility', 'visible');
                    $('.valid').css('visibility', 'hidden');
                }
                else {
                    $('.lgn_password').css('visibility', 'hidden');
                }

                debugger
                if (Valid) {
                    var login =
                        {
                            Username: $('#username').val().trim(),
                            Password: $('#password').val().trim()
                        }
                    debugger
                    $.ajax({
                        type: 'POST',
                        url: '/Home/login',
                        contentType: "application/json;charset=utf-8",
                        data: JSON.stringify({ Username: login.Username, Password: login.Password }),
                        success: function (data) {
                            if (data.status) {
                                window.location.href = (data.newurl);
                            }
                            else {

                                $('.valid').css('visibility', 'visible');
                                $('#username').focus();
                            }
                        }

                    })

                }
            })

            $('#signup').click(function () {

                var Valid = true;


                if (!($('#s_username').val().trim() != '')) {
                    Valid = false;
                    $('.s_username').css('visibility', 'visible');
                    $('#s_username').focus();
                }
                else {
                    $('.s_username').css('visibility', 'hidden');

                }


                if (!($('#s_password').val().trim() != '')) {
                    Valid = false;
                    $('.s_password').css('visibility', 'visible');

                }
                else {
                    $('.s_password').css('visibility', 'hidden');

                }





                if (!($('#User_email').val().trim() != '')) {
                    Valid = false;
                    $('.User_email').css('visibility', 'visible');


                }
                else {
                    $('.User_email').css('visibility', 'hidden');
                    var emailReg = /^([\w-\.]+@@([\w-]+\.)+[\w-]{2,4})?$/;
                    var emailval = $('#User_email').val();

                    if (emailReg.test(emailval)) {

                        $('.emailvalid').css('display', 'none');

                    }

                    else {
                        Valid = false;
                        $('.emailvalid').css('display', 'block');
                    }


                }


                if (!($('#User_contact').val().trim() != '')) {
                    Valid = false;
                    $('.User_contact').css('visibility', 'visible');

                }
                else {
                    $('.User_contact').css('visibility', 'hidden');

                }


                var vidFileLength = $("#file")[0].files.length;
                if (vidFileLength === 0) {
                    Valid = false;
                    $('.image').css('visibility', 'visible');
                }
                else {
                    $('.image').css('visibility', 'hidden');
                }

                var fileData = new FormData();
