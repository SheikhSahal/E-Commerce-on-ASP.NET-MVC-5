                    fileData.append("Username", $('#s_username').val().trim());
                    fileData.append("Password", $('#s_password').val().trim());
                    fileData.append("User_email", $('#User_email').val().trim());
                    fileData.append("User_contact", $('#User_contact').val());
                    fileData.append("file", files[0]);


                    debugger
                    $.ajax({
                        url: "/SignUp/Signup",
                        type: "POST",
